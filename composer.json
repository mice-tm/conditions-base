{
  "name": "mice-tm/conditions-base",
  "type": "library",
  "description": "Base library for conditons builder",
  "keywords": ["conditions", "builder"],
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Iryna Mychkova",
      "email": "imychkova@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "micetm\\conditionsBase\\": "src",
      "micetm\\conditionsBase\\tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "2.0.15.1",
    "yii2tech/embedded": "~1.0.3",
    "oomphinc/composer-installers-extender": "^1.1.2"
  },
  "config": {
    "process-timeout": 1800
  },
  "require-dev": {
    "codeception/codeception": "2.5.x-dev"
  }
}
